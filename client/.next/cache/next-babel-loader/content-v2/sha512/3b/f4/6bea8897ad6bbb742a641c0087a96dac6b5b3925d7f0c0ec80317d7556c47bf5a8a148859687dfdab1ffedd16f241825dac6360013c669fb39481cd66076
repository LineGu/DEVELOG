Intro] = useState<string>(introList[0]);\n\n  useEffect(() => {\n    let mounted = true;\n    function interactiveIntro() {\n      let i = 1;\n      setMargin('0%');\n      const timerToHide = setInterval(() => {\n        if (!mounted) {\n          clearInterval(timerToHide);\n          return;\n        }\n        setOpacity('0%');\n      }, 2000);\n      const timerToShow = setInterval(() => {\n        if (!mounted) {\n          clearInterval(timerToShow);\n          return;\n        }\n        setOpacity('100%');\n        setIntro(introList[i]);\n        i += 1;\n        i = i > 3 ? 0 : i;\n      }, 3000);\n    }\n    interactiveIntro();\n    return function cleanUp() {\n      mounted = false;\n    };\n  }, []);\n  return (\n    <StyledTextLoop margin={margin}>\n      <StyledSlideBox className=\"left\" />\n      <StyledSlideBox />\n      <StyledTextBox opacity={opacity}>{intro}</StyledTextBox>\n    </StyledTextLoop>\n  );\n}\n\nexport default TextLoop;\n"]},"metadata":{},"sourceType":"module"}