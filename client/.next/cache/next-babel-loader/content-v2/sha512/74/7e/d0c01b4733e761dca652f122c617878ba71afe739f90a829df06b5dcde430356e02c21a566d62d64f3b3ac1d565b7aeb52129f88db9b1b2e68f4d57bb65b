{"ast":null,"code":"import axios from 'axios';\nimport { headerOption } from '@config/headerForImgur';\nimport { url } from '@construction/url';\n\nconst uploadImg = async (file, setProcess) => {\n  const uploadInfo = {\n    data: '',\n    error: ''\n  };\n  const isExcessSize = file.size > 5000000;\n\n  const getUploadProgress = ProgressEvent => {\n    const progress = Math.round(ProgressEvent.loaded / ProgressEvent.total * 100);\n    setProcess({\n      process: progress,\n      error: ''\n    });\n  };\n\n  if (file && isExcessSize) {\n    setProcess({\n      process: 0,\n      error: 'EXCESS'\n    });\n  }\n\n  const imgData = new FormData();\n  imgData.append('image', file);\n\n  try {\n    const postConfig = {\n      headers: headerOption,\n      onUploadProgress: ProgressEvent => getUploadProgress(ProgressEvent)\n    };\n    const response = await axios.post(url.IMGUR, imgData, postConfig);\n    const imgLink = response.data.data.link;\n    uploadInfo.data = imgLink;\n    return uploadInfo;\n  } catch (error) {\n    setProcess({\n      process: 0,\n      error\n    });\n  }\n};\n\nexport default uploadImg;","map":{"version":3,"sources":["/Users/hyungu/Desktop/front-dev/projects/develog/client/src/utils/uploadImg.ts"],"names":["axios","headerOption","url","uploadImg","file","setProcess","uploadInfo","data","error","isExcessSize","size","getUploadProgress","ProgressEvent","progress","Math","round","loaded","total","process","imgData","FormData","append","postConfig","headers","onUploadProgress","response","post","IMGUR","imgLink","link"],"mappings":"AAAA,OAAOA,KAAP,MAAkB,OAAlB;AACA,SAASC,YAAT,QAA6B,wBAA7B;AACA,SAASC,GAAT,QAAoB,mBAApB;;AAQA,MAAMC,SAAS,GAAG,OAAOC,IAAP,EAAmBC,UAAnB,KAAyE;AACzF,QAAMC,UAAU,GAAG;AACjBC,IAAAA,IAAI,EAAE,EADW;AAEjBC,IAAAA,KAAK,EAAE;AAFU,GAAnB;AAIA,QAAMC,YAAY,GAAGL,IAAI,CAACM,IAAL,GAAY,OAAjC;;AAEA,QAAMC,iBAAiB,GAAIC,aAAD,IAAkC;AAC1D,UAAMC,QAAQ,GAAGC,IAAI,CAACC,KAAL,CAAYH,aAAa,CAACI,MAAd,GAAuBJ,aAAa,CAACK,KAAtC,GAA+C,GAA1D,CAAjB;AACAZ,IAA