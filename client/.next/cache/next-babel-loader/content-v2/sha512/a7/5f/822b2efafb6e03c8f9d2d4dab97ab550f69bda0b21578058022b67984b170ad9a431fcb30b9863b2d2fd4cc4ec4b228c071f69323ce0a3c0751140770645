tonProps): ReactElement {\n  const { input, setInput, inputAreaElem, setUploadState, moveCursor, cursorPosition } = editButtonProps;\n  const [isHiddenTableModal, setIsHidden] = useState<boolean>(true);\n\n  const controlTableModal: IOnClickSvgFunc = (event) => {\n    setIsHidden(!isHiddenTableModal);\n    event.stopPropagation();\n  };\n\n  const onClick: IOnClickSvgFunc = (event) => {\n    const editButton: string = event.currentTarget.id;\n    const currentCursorIndex = cursorPosition[0];\n    const updatedInput = Editors[editButton].onClickEditButton({ input, currentCursorIndex, moveCursor });\n    setInput(updatedInput);\n  };\n\n  return (\n    <StyledEditButtonBox>\n      <H1 onClick={onClick} />\n      <H2 onClick={onClick} />\n      <H3 onClick={onClick} />\n      <Bold onClick={onClick} />\n      <Italic onClick={onClick} />\n      <Link onClick={onClick} />\n      <ImgButton setUploadState={setUploadState} />\n      <Quote onClick={onClick} />\n      <TableButton />\n      <Code onClick={onClick} />\n      <CheckBox onClick={onClick} />\n    </StyledEditButtonBox>\n  );\n}\n\nexport default EditButtonBox;\n"]},"metadata":{},"sourceType":"module"}