psForTagArea }: ITagAreaProps): ReactElement {\n  const { tagList, setTagList, setModalState } = propsForTagArea;\n\n  const addTag: IOnKeyboard = (event, setTagInput, tagInput: string) => {\n    const isNotValidTag = !event || event.currentTarget.value.length < 2;\n    if (isNotValidTag) return;\n    const isRequestToAdd = event.key === ' ' || event.key === 'Enter';\n    if (isRequestToAdd) {\n      const tagListCopied: string[] = [...tagList];\n      tagListCopied.push(tagInput.slice(1));\n      setTagList(tagListCopied);\n      setTagInput('');\n      setModalState('hidden');\n    }\n  };\n\n  return (\n    <TextAreaForTag\n      placeholder={message.TAG_PLACEHOLDER}\n      onFocus={() => setModalState('show')}\n      onBlur={() => setModalState('hidden')}\n      onChange={renderTagChanged}\n      onKeyDown={addTag}\n    />\n  );\n}\n\nexport default PostTagInput;\n"]},"metadata":{},"sourceType":"module"}